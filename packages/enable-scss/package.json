{
	"repository": {
		"url": "https://github.com/remotion-dev/remotion/tree/main/packages/enable-scss"
	},
	"name": "@remotion/enable-scss",
	"version": "4.0.340",
	"description": "Enable SCSS support in Remotion",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.mjs",
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src",
		"make": "tsc -d && bun --env-file=../.env.bundle bundle.ts",
		"formatting": "prettier --experimental-cli src --check"
	},
	"author": "Jonny Burger <jonny@remotion.dev>",
	"contributors": [],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"dependencies": {
		"style-loader": "4.0.0",
		"css-loader": "5.2.7",
		"sass-loader": "14.2.1"
	},
	"peerDependencies": {
		"@remotion/bundler": "workspace:*"
	},
	"devDependencies": {
		"@remotion/bundler": "workspace:*",
		"@remotion/eslint-config-internal": "workspace:*",
		"eslint": "9.19.0"
	},
	"keywords": [
		"remotion",
		"tailwind"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/cjs/index.d.ts",
			"module": "./dist/esm/index.mjs",
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"homepage": "https://www.remotion.dev/docs/enable-scss/overview"
}
