{
  "extends": "../tsconfig.settings.json",
  "baseUrl": ".",
  "compilerOptions": {
    "types": ["node"],
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "target": "ES2019",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strict": false,
    "noEmit": true
  },
  "paths": {
    "@site/*": ["./*"]
  },
  "types": [
    "node",
    "@docusaurus/module-type-aliases",
    "@docusaurus/theme-classic"
  ],
  "references": [
    { "path": "../player" },
    { "path": "../core" },
    { "path": "../media-utils" },
    { "path": "../babel-loader" },
    { "path": "../cloudrun" },
    { "path": "../create-video" },
    { "path": "../fonts" },
    { "path": "../lottie" },
    { "path": "../noise" },
    { "path": "../paths" },
    { "path": "../google-fonts" },
    { "path": "../preload" },
    { "path": "../rive" },
    { "path": "../skia" },
    { "path": "../three" },
    { "path": "../shapes" },
    { "path": "../gif" },
    { "path": "../motion-blur" },
    { "path": "../lambda" },
    { "path": "../bundler" },
    { "path": "../renderer" },
    { "path": "../transitions" },
    { "path": "../cli" }
  ]
}
