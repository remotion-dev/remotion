			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"version": "4.0.340",
	"repository": {
		"url": "https://github.com/remotion-dev/remotion/tree/main/packages/svg-3d-engine"
	},
	"description": "3D SVG extrusion effects",
	"publishConfig": {
		"access": "public"
	}
