{
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
		"declaration": true,
		"declarationMap": false,
		"sourceMap": false,
		"lib": ["es2023", "dom"],
		"jsx": "react-jsx",
		"noEmit": false,
		"skipLibCheck": true,
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"composite": true
	}
}
